{
  "sport_name": "example_sport",
  "data_file": "path/to/your_data.csv",
  "csv_columns": [
    "entity1_name",
    "entity2_name",
    "date",
    "action_type",
    "location",
    "direction",
    "outcome"
  ],
  "entity_name": "player",
  "date_column": "date",
  "entity_columns": ["entity1_name", "entity2_name"],
  "lookback_years": 2,
  "var_template": "templates/example_var.txt",
  "assertion_template": "#define entity1Win won == entity1;\n#assert Game reaches entity1Win with prob;",
  "variants": [
    {
      "name": "default",
      "template_file": "templates/example_model.txt",
      "applies_when": {},
      "parameter_groups": [
        {
          "name": "Entity 1 Actions",
          "filter_query": "action_type == 1",
          "count_queries": {
            "action_a_success": "outcome == 1",
            "action_b_success": "outcome == 2",
            "winner": "outcome == 3",
            "error": "outcome == 4"
          }
        },
        {
          "name": "Entity 1 Responses",
          "filter_query": "action_type == 2",
          "count_queries": {
            "action_a_success": "outcome == 1",
            "action_b_success": "outcome == 2",
            "winner": "outcome == 3",
            "error": "outcome == 4"
          }
        }
      ]
    }
  ]
}
